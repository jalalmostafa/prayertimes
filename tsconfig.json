{
  "compilerOptions": {
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noEmitHelpers": false,
    "module": "es2015",
    "moduleResolution": "node",
    "pretty": true,
    "target": "es2015",
    "jsx": "react-jsx",
    "esModuleInterop": true
  },
  "include": [
    "./app/**/*"
  ]
}
